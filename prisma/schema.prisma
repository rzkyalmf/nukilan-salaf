generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Quote {
  id String @id @default(cuid())
  title String
  author String
  content String
  source String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}